<body class="{{ body_class }}">
  

    {% block navigation %}

        {% include "includes/header.j2" %}
            
    {% endblock %}
    

    {% block sub_navigation %}
    {% endblock %}

    {% block header %}
    {% endblock %}

    {% block content %}

        {% for item in data %}
            {{ item|component }}
        {% endfor %}

        {% block page_site_wrapper %}


            {% block page_navigation %}
            {% endblock %}


            {% block page_sub_navigation %}
            {% endblock %}


            {% block page_content %}
            {% endblock %}


            {% block page_footer %}
            {% endblock %}

        {% endblock %}

    {% endblock %}



    {% block footer %}

        {% include "/includes/footer.j2" %}

    {% endblock %}

    {% block anchor %}
